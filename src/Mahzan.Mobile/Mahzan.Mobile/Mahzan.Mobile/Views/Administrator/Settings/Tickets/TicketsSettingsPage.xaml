<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Mahzan.Mobile.Views.Administrator.Settings.Tickets.TicketsSettingsPage">
    <ContentPage.Content>
        <StackLayout>
            <Button 
                    Text="Imprimir"
                    BackgroundColor="SteelBlue"
                    BorderRadius="20"
                    TextColor="White"
                    FontAttributes="Bold"
                    Command="{Binding PrintTicketCommand}">
            </Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>